package com.org.dataStructuresCources;

public class BubbleSortImpl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static int[] swap(int[] array, int i, int j) {
		if(i==j) {
			return array;
		}
		int temp = array[i];
		array[i] = array[j];
		array[j] = temp;
		return array;
	}

}
